{{outlet}}

<div class='navbar navbar-inverse navbar-fixed-bottom'>
  <button type="button" id='footButton' class="btn btn-primary" data-toggle="modal" data-target=".bs-example-modal-lg">Add tree</button>
</div>

<div id="new-tree">
  {{new-tree save2="save3"}}
</div>

<div class=container>
  <div class='row'>

  <div id="trees">
    {{#each model as |tree|}}
      <hr>
      {{tree-tile tree=tree update="update" destroyPost="destroyPost"}}
    {{/each}}
  </div>
</div>
</div>
